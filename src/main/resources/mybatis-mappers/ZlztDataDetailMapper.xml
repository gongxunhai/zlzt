<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.security.server.dao.ZlztDataDetailDao">

	<sql id="where">
		<where>
			<if test="params.id != null and params.id != ''">
				and id = #{params.id} 
			</if>
			<if test="params.dataId != null and params.dataId != ''">
				and dataId = #{params.dataId} 
			</if>
			<if test="params.fId != null and params.fId != ''">
				and fId = #{params.fId} 
			</if>
			<if test="params.sId != null and params.sId != ''">
				and sId = #{params.sId} 
			</if>
			<if test="params.tId != null and params.tId != ''">
				and tId = #{params.tId} 
			</if>
			<if test="params.cId != null and params.cId != ''">
				and cId = #{params.cId} 
			</if>
			<if test="params.createTime != null and params.createTime != ''">
				and createTime = #{params.createTime} 
			</if>
			<if test="params.updateTime != null and params.updateTime != ''">
				and updateTime = #{params.updateTime} 
			</if>
		
		</where>
	</sql>

	<select id="count" resultType="int">
		select count(1) from zlzt_data_detail t
		<include refid="where" />
	</select>

	<select id="list" resultType="ZlztDataDetail">
		select * from zlzt_data_detail t
		<include refid="where" />
		${params.orderBy}
		limit #{offset}, #{limit}
	</select>

	<update id="update">
		update zlzt_data_detail t
		<set>
			<if test="dataId != null">
				dataId = #{dataId}, 
			</if>
			<if test="fId != null">
				fId = #{fId}, 
			</if>
			<if test="sId != null">
				sId = #{sId}, 
			</if>
			<if test="tId != null">
				tId = #{tId}, 
			</if>
			<if test="cId != null">
				cId = #{cId}, 
			</if>
			<if test="createTime != null">
				createTime = #{createTime}, 
			</if>
			<if test="updateTime != null">
				updateTime = #{updateTime}, 
			</if>

		</set>

		where t.id = #{id}
	</update>

</mapper>
